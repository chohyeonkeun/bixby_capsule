capsule {
  id (jonus.search)
  version (0.1.0)
  format (3)
  targets {
    target (bixby-mobile-ko-KR)
  }
  runtime-flags {
    use-input-views-for-selection-list-detail
    modern-default-view-behavior
    no-filtering-with-validation
    concepts-inherit-super-type-features
    modern-prompt-rejection
    support-halt-effect-in-computed-inputs
  }
  capsule-imports {
    import (viv.entity) {
      as (entity)
      version (3.19.8)
    }
    import (viv.core){
      as (core)
      version-from (entity)
    }
    import (viv.image){
      as (image)
      version-from (entity)
    }
    import (viv.time) {
      as (time)
      version-from (entity)
    }
  }
}
