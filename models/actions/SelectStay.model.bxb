action (SelectStay) {
  type (Search)
  description (사용자가 원하는 숙소 상세 페이지 이동)
  collect {
    input (stayId) {
      type (StayId)
      min (Required) max (One)
    }
    input (startDate) {
      type (StartDate)
      min (Required) max (One)
    }
    input (endDate) {
      type (EndDate)
      min (Required) max (One)
    }
  }
  output (Stay) 
}
