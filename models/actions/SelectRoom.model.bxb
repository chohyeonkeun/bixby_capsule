action (SelectRoom) {
  type (Search)
  description (사용자가 방 선택하여 세부 화면으로 이동)
  collect {    
    input (roomId) {
      type (RoomName)
      min (Required) max (One)
    }
    input (startDate) {
      type (StartDate)
      min (Required) max (One)
    }
    input (endDate) {
      type (EndDate)
      min (Required) max (One)
    }
  }
  output (Room)
}
