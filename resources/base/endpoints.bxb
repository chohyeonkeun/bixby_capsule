endpoints {
  action-endpoints {
    action-endpoint (FindStay) {
      accepted-inputs (searchKeyword, popularKeyword, category, priceLow, priceHigh, review, wish, stayId, dateInterval)
      local-endpoint (FindStay.js)
    }
    action-endpoint (SelectRoom) {
      accepted-inputs (roomId, dateInterval)
      local-endpoint (SelectRoom.js)
    }
  }
}